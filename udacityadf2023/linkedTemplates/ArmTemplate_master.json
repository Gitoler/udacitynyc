{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "udacityadf2023"
		},
		"UdacityDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'UdacityDb'"
		},
		"UdacityLake2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'UdacityLake2'"
		},
		"UdacityPool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'UdacityPool'"
		},
		"UdacityLake2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://udacitynyc2023.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "udacityadf2023_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"UdacityDb_connectionString": {
						"value": "[parameters('UdacityDb_connectionString')]"
					},
					"UdacityLake2_accountKey": {
						"value": "[parameters('UdacityLake2_accountKey')]"
					},
					"UdacityPool_connectionString": {
						"value": "[parameters('UdacityPool_connectionString')]"
					},
					"UdacityLake2_properties_typeProperties_url": {
						"value": "[parameters('UdacityLake2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "udacityadf2023_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'udacityadf2023_ArmTemplate_0')]"
			]
		}
	]
}